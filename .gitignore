# Ignore back-up files.
*~

# Ignore compiled Python files.
*.pyc
*.pyo

# Don't include build related files.
/dependencies/
/dist/
/build/
*/_build/
/_sources/
/.tox

# And don't care about the 'egg'.
/turbinia.egg-info
.eggs/

# Test files
.coverage
tests-coverage.txt

# Exclude Terraform runtime files
terraform/gcp/.terraform/
terraform/gcp/terraform.tfstate
terraform/gcp/terraform.tfstate.backup
terraform/gcp/terraform.tfvars
terraform/gcp/*.auto.tfvars

# Exclude IDE files
.idea/
.vscode/

# Exclude  logs
*.log
*.log.gz

# Exclude Python virtualenvs
venv/
.venv/

# Ignore OpenAPI code generator temporary files
.openapi-generator/
